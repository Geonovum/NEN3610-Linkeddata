@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix geo: <http://www.opengis.net/ont/geosparql#> .

@prefix imgolf: <http://linkeddata.ordina.nl/nen3610/def/imgolf#>.
@prefix concept: <http://linkeddata.ordina.nl/nen3610/imgolf/id/concept/>.

@prefix golfbaan: <http://linkeddata.ordina.nl/id/golfbaan/>.
@prefix parcours: <http://linkeddata.ordina.nl/id/parcours/>.
@prefix tee: <http://linkeddata.ordina.nl/id/tee/>.
@prefix hole: <http://linkeddata.ordina.nl/id/hole/>.
@prefix parcoursdeel: <http://linkeddata.ordina.nl/id/parcoursdeel/>.
@prefix geometrie: <http://linkeddata.ordina.nl/id/geometrie/>.

#Overgenomen referentiegegevens
concept:Openbaar a skos:Concept;
  rdfs:label "openbaar";
  skos:prefLabel "openbaar"@nl;
  skos:inScheme schema:IMGolf;
  skos:definition "Toegankelijk voor iedereen"@nl;
  skos:inScheme schema:IMGolf
.
concept:LageNatuurwaarde a skos:Concept;
  rdfs:label "lage natuurwaarde";
  skos:inScheme schema:IMGolf;
  skos:prefLabel "laag";
.
<http://brt.basisregistraties.overheid.nl/id/begrip/InGebruik> a skos:Concept;
  rdfs:label "in gebruik";
  skos:definition "Het object wordt gebruikt.";
.

#Overgenomen uit model
imgolf:Green rdfs:subClassOf imgolf:Parcoursdeel.
imgolf:Fairway rdfs:subClassOf imgolf:Parcoursdeel.

#Overgenomen uit model: dit zou eigenlijk afgeleid moeten worden!!
imgolf:Parkbaan rdfs:subClassOf imgolf:Golfbaan.
imgolf:Polderbaan rdfs:subClassOf imgolf:Golfbaan.

# Data
golfbaan:Hoogland a imgolf:Golfbaan;
  # a imgolf:Parkbaan, imgolf:Polderbaan;
  geo:hasGeometry geometrie:GolfbaanHoogland;
  imgolf:naam "Golfbaan hoogland"@nl;
  imgolf:toegang [
    imgolf:toegankelijkheid concept:openbaar;
    imgolf:vereisten "Lid of introduce";
    imgolf:etiquette "Algemene etiquette";
  ];
  imgolf:natuurwaarde concept:LageNatuurwaarde;
  imgolf:gebruiksstatus <http://brt.basisregistraties.overheid.nl/id/begrip/InGebruik>;
  imgolf:hole parcours:HooglandHole1;
  imgolf:hole parcours:HooglandHole2;
  imgolf:hole parcours:HooglandHole3;
  imgolf:hole parcours:HooglandHole4;
  imgolf:hole parcours:HooglandHole5;
  imgolf:hole parcours:HooglandHole6;
  imgolf:hole parcours:HooglandHole7;
  imgolf:hole parcours:HooglandHole8;
  imgolf:hole parcours:HooglandHole9;
.
geometrie:GolfbaanHoogland a geo:Geometry;
  geo:hasWKT "POINT (12 52)"^^geo:wktLiteral;
.

parcours:HooglandHole1 a imgolf:Parcours;
  geo:hasGeometry geometrie:ParcoursHooglandHole1;
  imgolf:nummer 1;
  imgolf:par 3;
  imgolf:afslagplaats tee:HooglandHole1Tee;
  imgolf:parcoursDeel parcoursdeel:HooglandHole1Fairway;
  imgolf:parcoursDeel parcoursdeel:HooglandHole1Green;
.
geometrie:ParcoursHooglandHole1 a geo:Geometry;
  geo:hasWKT "POINT (12 52)"^^geo:wktLiteral;
.

tee:HooglandHole1Tee a imgolf:Tee;
  geo:hasGeometry geometrie:HooglandHole1Tee;
.
geometrie:HooglandHole1Tee a geo:Geometry;
  geo:hasWKT "POINT (12 52)"^^geo:wktLiteral;
.

parcoursdeel:HooglandHole1Fairway a imgolf:Fairway;
  geo:hasGeometry geometrie:HooglandHole1Fairway;
.
geometrie:HooglandHole1Fairway a geo:Geometry;
  geo:hasWKT "POINT (12 52)"^^geo:wktLiteral;
.

parcoursdeel:HooglandHole1Green a imgolf:Green;
  geo:hasGeometry geometrie:HooglandHole1Green;
  geo:hole hole:HooglandHole1;
.
geometrie:HooglandHole1Green a geo:Geometry;
  geo:hasWKT "POINT (12 52)"^^geo:wktLiteral;
.

hole:HooglandHole1 a imgolf:Hole;
  geo:hasGeometry geometrie:HoleHooglandHole1;
.
geometrie:HoleHooglandHole1 a geo:Geometry;
  geo:hasWKT "POINT (12 52)"^^geo:wktLiteral;
.
