stage:MetamodelDiagram	a	elmo:Representation;
	elmo:url-pattern	"/query/metamodel-diagram$";
	elmo:contains	stage:Menu;
	elmo:appearance elmo:ImageAppearance;
	elmo:query '''
		prefix html: <http://www.w3.org/1999/xhtml/vocab#>
		prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>
		construct {
			<urn:image> html:img "https://github.com/Geonovum/NEN3610-Linkeddata/raw/gh-pages/LD%20modellen/visualisaties/metamodel.png".
			<urn:image> html:width "1185".
			<urn:image> html:height "569".
			<urn:image> html:top "0".
			<urn:image> html:left "0".
		}
		where {
		}
	'''
.

